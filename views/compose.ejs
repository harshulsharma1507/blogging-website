<%- include('partials/header') %> 
<h1>Compose New Post</h1>
<br>
<div class="form-group"></div>
<form action="/compose" method="post">
<label for="title">Title</label><br>
<input class="form-control" id="title "type="text" name="title"><br>
<label for="post">Post</label><br>
<textarea class="form-control" name="content" id="post" cols="100" rows="10"></textarea><br>
<button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>
<%- include('partials/footer') %>